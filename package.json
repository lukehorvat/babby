{
  "name": "babby",
  "version": "0.0.0",
  "description": "A thin wrapper around Babel v6 to enable advanced plugins.",
  "author": "Luke Horvat",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/lukehorvat/babby.git"
  },
  "bugs": {
    "url": "https://github.com/lukehorvat/babby/issues"
  },
  "main": "dist/babel-register.js",
  "bin": {
    "babel": "dist/babel.js",
    "babel-node": "dist/babel-node.js"
  },
  "scripts": {
    "build": "rimraf dist && babel lib -d dist",
    "prepublish": "npm run build"
  },
  "dependencies": {
    "babel-cli": "6.7.7",
    "babel-core": "6.7.7",
    "babel-register": "6.7.2",
    "babylon": "6.7.0",
    "overwrite": "1.0.3"
  },
  "devDependencies": {
    "babel-preset-es2015": "6.6.0",
    "rimraf": "2.5.2"
  },
  "keywords": [
    "babel"
  ],
  "babel": {
    "presets": [
      "es2015"
    ]
  }
}
